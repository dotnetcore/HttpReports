<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>HttpReports.PostgreSQL</PackageId>
    <Version>2.0.0</Version>
    <Authors>SpringLee</Authors>
    <Company>SpringLee</Company>
    <Description>HttpReports is a lightweight APM system developed for Net Core </Description>
    <PackageProjectUrl>https://github.com/dotnetcore/HttpReports</PackageProjectUrl>
    <RepositoryUrl>https://github.com/dotnetcore/HttpReports</RepositoryUrl> 
    <AssemblyName>HttpReports.Storage.PostgreSQL</AssemblyName>
    <Product>HttpReports.PostgreSQL</Product>
    <PackageIcon>logo3.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FreeSql" Version="1.8.1" />
    <PackageReference Include="FreeSql.Provider.PostgreSQL" Version="1.8.1" />
    <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="2.1.1" />
    <PackageReference Include="Npgsql" Version="4.1.4" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\HttpReports.Core\HttpReports.Core.csproj" />
    <ProjectReference Include="..\HttpReports.Storage.Abstractions\HttpReports.Storage.Abstractions.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\HttpReports.Dashboard.UI\static\logo3.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
